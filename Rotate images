class Solution {
    public void rotate(int[][] matrix) {
        Stack<Integer> st=new Stack<>();
        int n=matrix.length;
        for(int i=0;i<n;i++){
            for(int j=n-1;j>=0;j--){
                st.push(matrix[i][j]);
                }
            }
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                int x=st.pop();
                matrix[j][i]=x;
                }
            }
    }
}
